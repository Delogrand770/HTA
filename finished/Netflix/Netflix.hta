<html>
<head><title>Netflix</title>
<link rel=StyleSheet href="xcss.css" type="text/css">
<script src="gaia.js"></script>
<style>
body{
  font-size: 11;
  padding: 0;
  margin: 0;
  overflow: hidden;
  font-family:arial,sans-serif;
}
</style>
<script>
var App = {
	Tidy:function(){
		frames['cont1'].onerror = function(){return false;}
		//setTimeout("XU.ResizeWindow(600,1124,0,1);",3000);
	},
	Ini:function(){
		XI.New('area','flix','1:1','800:1400','block','silver',100,'silver');
		XI.Fill('flix','Netflix','<iframe src="http://www.netflix.com" trusted="yes" application="yes" style="height:100%;width:100%;" onload="App.Tidy();" id="cont1" name="cont1"></iframe>',1);
	}
};
</script>
</head>
<body onload="App.Ini()">
<HTA:APPLICATION ID="netflix"
    APPLICATIONNAME="netflix"
    BORDER="normal"
    BORDERSTYLE="normal"
    CAPTION="yes"
    ICON="favicon.ico"
    SCROLL="no"
    MAXIMIZEBUTTON="yes"
    MINIMIZEBUTTON="yes"
    INNERBORDER="no"
    CONTEXTMENU="no"
    SINGLEINSTANCE="yes"
    WINDOWSTATE="normal"
    SYSMENU="yes"
    SHOWINTASKBAR="yes"
    SCROLLFLAT="yes"
    NAVIGABLE="no"
    SELECTION="yes">
<span id="area"></span>
</body>
</html>