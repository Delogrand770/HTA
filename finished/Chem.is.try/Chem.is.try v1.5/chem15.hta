<script>
XUtil.ResizeWindow(720,1024,0,1);
var cz       = 0;
var datalist = new Array('element_table','acid_table','ions_table','nonionic_table','ionic_table','alkanes_table');
var qsets    = new Array('topic','question type','question number');
var Sys = {
	Ini:function(){
		XMessage.Ini();
		Sys.SplashScreen(1);
		setTimeout("Sys.SplashScreen(0);",2000);
	},
	SplashScreen:function(it){
		if (it==1){
			XMessage.Alert('<font color="#0099ff">Chem.is.try</font>','<font size=2em color=white>Created by Gavin Delphia and Mathieu Lefebvre<br><br>Prep class 2010 / USAFA class 2014</font>',1);
		}else{
			Sys.MainMenu();
		}
	},
	MainMenu:function(){
			XMessage.Close();
			backbtn.style.display       = "none";
			span_quiztype.style.display = "none";
			span_mainmenu.style.display = 'block';
			XUtil.CenterElement('span_mainmenu','b');
	},
	QuizPrep:function(it,it2){
		if (it!=-1){
			span_mainmenu.style.display    = "none";
			span_quiztype_header.innerHTML = event.srcElement.innerHTML;
			backbtn.style.display          = "block";
			span_quiztype.style.display    = "block";
			XUtil.CenterElement('span_quiztype','b');
			qsets[2] = 0;
			qsets[0] = it;
			
		}
		else if (it==-1 && it2!=-1){
			qsets[1] = it2;
			cz = new Quiz.Question(qsets[0],qsets[1]);
			cz.Status();
		}
	}
};
var Quiz = {
	Question:function(itopic,iqtype){
		this.topic  = itopic; //0elements, 1acids, 2ions, 3covalent.c, 4ionic.c, 5alkanes
		this.qtype  = iqtype; //0cn, 1tn, 2cs, 3ts

		if (this.qtype == 0 || this.qtype == 1){
			
		}
		else if (this.qtype == 2 || this.qtype == 3){
			window[datalist[this.topic]]
		}

		
		this.Status = function(){
			alert('topic: '+this.topic+'\nqtype: '+this.qtype);
		}
	}

};
</script>
</head>
